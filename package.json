    "date-fns": "3.6.0",
    "glyph-segregator": "0.6.5",
    "hast-util-select": "5.0.5",
    "iles": "0.9.5",
    "isbot": "5.1.6",
    "katex": "0.16.10",
    "mdast-util-to-hast": "12.3.0",
    "plausible-tracker": "0.3.8",
    "puppeteer": "22.8.0",
    "rehype-plugin-image-native-lazy-loading": "1.2.0",
    "shiki": "0.14.7",
    "simple-git": "3.24.0",
    "sitemap": "7.1.1",
    "ts-exhaustive-check": "1.0.0",
    "unified": "10.1.2",
    "unist-util-visit": "5.0.0",
