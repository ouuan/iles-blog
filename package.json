    "remark-gfm": "3.0.1",
    "remark-math": "5.1.1",
    "remark-parse": "10.0.2",
    "rollup-plugin-analyzer": "4.0.0",
    "sass": "1.77.1",
    "tsup": "8.0.2",
    "typescript": "5.4.5",
    "vite-plugin-compression": "0.5.1",
    "vue-tsc": "2.0.17"
  },
  "pnpm": {
    "overrides": {
      "vite-plugin-image-presets": "0.3.4",
      "puppeteer": "$puppeteer",
      "array-includes": "npm:@nolyfill/array-includes@latest",
